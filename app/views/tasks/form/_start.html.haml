= form_for(task, remote: true, url: task_status_path(task)) do |f|
  = f.hidden_field :started_at, value: Time.now.to_s(:db)
  = f.hidden_field :status, value: 'doing' 
  = f.hidden_field :week_id, value: nil
  = f.button " #{content_tag(:i, '', class: 'fa fa-play-circle')}START".html_safe, type: :submit
